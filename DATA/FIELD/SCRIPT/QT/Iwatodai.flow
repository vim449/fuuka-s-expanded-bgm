void gotoOctopia()
{
	int var1;
	OPEN_MSG_WIN();
	if ( BIT_CHK( 257 ) == 0 )
	{
		MSG( JUNPEI_STOP_TAKOYAKI );
	}
	else 
	{
		MSG( TAKOYAKI_SHONITI );
		MSG( TAKOYAKI_SENTAKU );
		if ( SEL( TAKOYAKI_SEL ) == 0 )
		{
			if ( GET_YEN() < 400 )
			{
				MSG( MONEY_TARINAI );
				MSG( TAKOYAKI_KAWANAI );
			}
			else 
			{
				var1 = GET_ITEM( 1, ( 4000 + 108 ) );
				if ( var1 != 99 )
				{
					var1 = ( var1 + 1 );
				}
				SET_ITEM( 1, ( 4000 + 108 ), var1 );
				SET_MSG_VAR( 0, ( 4000 + 108 ), 3 );
				MSG( TAKOYAKI_OWARI );
				REMOVE_YEN( 400 );
			}
		}
		else 
		{
			MSG( TAKOYAKI_KAWANAI );
		}
	}
	CLOSE_MSG_WIN();
	return;
}
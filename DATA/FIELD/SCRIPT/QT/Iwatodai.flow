void IwatodaiStation()
{
	switch(GET_FIELD_MINOR())
	{
		case 1: // Iwatodai Strip, Ground Floor
			if(Chk_Access_IwatodaiStation_001_002())
			{
				gotoIwatodaiStation();
			}
			break;
		case 2: // Iwatodai Station
			OPEN_MSG_WIN();
			MSG( Same );
			CLOSE_MSG_WIN();
			break;
		case 7: // Iwatodai Strip, Upper Floor
			if(Chk_Access_IwatodaiStation_007_001())
			{
				if(Chk_Access_IwatodaiStation_001_002())
				{
					gotoIwatodaiStation();
				}
			}
			break;
		default:
			No_QuickTravel();
			break;
	}
	return;
}

void gotoIwatodaiStation()
{
	FADE( 2, 0 );
	FADE_SYNC();
	CALL_FIELD( 9, 2, 0, 0 );
	return;
}


void get_out_here()
{
    OPEN_MSG_WIN();
    
    if ( ( ( GET_MONTH() == 4 ) && ( GET_DAY_OF_MONTH() == 20 ) ) && ( BIT_CHK( 35 ) == 0 ) )
    {
        MSG( EXIT_JUNPEI_STOP );
    }
    else if ( ( GET_MONTH() == 4 ) && ( GET_DAY_OF_MONTH() == 20 ) )
    {
        MSG( HELP_GET_OUT_HERE1 );
        CLOSE_MSG_WIN();
        MSG_HELP( HELP_GET_OUT_HERE2 );
        OPEN_MSG_WIN();
        MSG( HELP_GET_OUT_HERE3 );
        SEL_CHK_PAD( 6, 1 );
        
        if ( SEL( OUTDOOR_SEL ) == 0 )
        {
            MSG( END_EXPLORATION );
            CLOSE_MSG_WIN();
            FUNCTION_0186( 15 );
            FADE( 5, 30 );
            FADE_SYNC();
            BIT_ON( 3502 );
            call_myroom();
            return;
        }

        CLOSE_MSG_WIN();
        return;
    }
    else if ( ( ( GET_MONTH() == 4 ) && ( GET_DAY_OF_MONTH() == 30 ) ) && ( BIT_CHK( 3488 ) == 1 ) )
    {
        MSG( STOP_MITSURU_0430 );
    }
    else if ( ( GET_MONTH() == 1 ) && ( GET_DAY_OF_MONTH() == 31 ) )
    {
        MSG( TEST_LAST_DAY );
    }
    else 
    {
        MSG( HELP_GET_OUT_HERE3 );
        SEL_CHK_PAD( 6, 1 );
        
        if ( SEL( OUTDOOR_SEL ) == 0 )
        {
            MSG( END_EXPLORATION );
            CLOSE_MSG_WIN();
            FUNCTION_0186( 15 );
            FADE( 5, 30 );
            FADE_SYNC();
            call_myroom();
            return;
        }

    }
    CLOSE_MSG_WIN();
}